{
  "name": "@aptos-labs/aptos-cli",
  "version": "0.0.11",
  "description": "Aptos CLI available from npmjs",
  "bin": {
    "aptos": "./bin/aptos"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "node dist/index.js",
    "build": "rm -rf dist && tsup --platform node --format cjs,esm --dts"
  },
  "dependencies": {
    "aptos": "latest"
  },
  "devDependencies": {
    "@types/node": "^16.18.11",
    "ts-node": "10.9.1",
    "typescript": "^5.1.6",
    "tsup": "6.2.3"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aptos-labs/aptos-core.git"
  },
  "homepage": "https://github.com/aptos-labs/aptos-core",
  "bugs": {
    "url": "https://github.com/aptos-labs/aptos-core/issues"
  },
  "author": "aptoslabs.com",
  "keywords": [
    "Aptos",
    "Aptos Labs",
    "Aptos CLI"
  ],
  "license": "Apache-2.0"
}
