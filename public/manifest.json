{
  "short_name": "Aptos",
  "name": "Aptos Wallet",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "contentscript.js"
    ],
    "run_at": "document_start"
  }],
  "web_accessible_resources": [{
    "resources": ["inpage.js"],
    "matches": ["<all_urls>"]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
]
}
